$('#dialog h3').html("<i class='glyphicon glyphicon-plus'></i> <%= params[:date].to_date.strftime("%m월 %d일") %>의 활동 기록하기");
$('.modal-body').html('<%= j render 'form' %>');
$('#dialog').modal("show");
$('label:has(input:radio:checked)').addClass('active');
$('label:has(input:radio:not(:checked))').removeClass('active');